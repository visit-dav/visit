##############################################################################

##
## Programs and options...
##
@SET_MAKE@
@VARIABLES@

CXXFLAGS=-I../../include -I../../include/visit @CXXFLAGS@
CPPFLAGS=-I. @CPPFLAGS@ -DLAUNCHER_RPC_EXPORTS

##
## Files...
##
SRC=LaunchRPC.C ConnectSimRPC.C
HDR=LaunchRPC.h ConnectSimRPC.h

OBJ=$(SRC:.C=.o)
LIB=libvclrpc
LIBS=-lmisc -lstate -lcomm -lutility


##
## Standard targets...
##
all: $(LIBSHARED)

clean:
	$(RM) $(LIBSHARED)
	$(RM) $(OBJ)

##
## Automatic dependency stuff
##
@TARGETS@
@LIBTARGETS@
