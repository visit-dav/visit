	$(CC) -o $(PROGRAM_UNSTRUCTURED2D) $(OBJ_UNSTRUCTURED2D) $(LDFLAGS) $(LIBS)

$(FPROGRAM_UNSTRUCTURED2D): $(FOBJ_UNSTRUCTURED2D)
	$(F77) -o $(FPROGRAM_UNSTRUCTURED2D) $(FOBJ_UNSTRUCTURED2D) $(LDFLAGS) $(LIBS)

