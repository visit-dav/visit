	$(F77) -o $(FPROGRAM_CURV3D) $(FOBJ_CURV3D) $(LDFLAGS) $(LIBS)

$(PROGRAM_POINT2D): $(OBJ_POINT2D)
	$(CC) -o $(PROGRAM_POINT2D) $(OBJ_POINT2D) $(LDFLAGS) $(LIBS)

$(FPROGRAM_POINT2D): $(FOBJ_POINT2D)
	$(F77) -o $(FPROGRAM_POINT2D) $(FOBJ_POINT2D) $(LDFLAGS) $(LIBS)

