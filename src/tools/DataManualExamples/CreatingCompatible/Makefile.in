	$(F77) -o $(FPROGRAM_UNSTRUCTURED3D) $(FOBJ_UNSTRUCTURED3D) $(LDFLAGS) $(LIBS)

$(PROGRAM_UCDVAR2D): $(OBJ_UCDVAR2D)
	$(CC) -o $(PROGRAM_UCDVAR2D) $(OBJ_UCDVAR2D) $(LDFLAGS) $(LIBS)

