##
## Programs and options...
##
@SET_MAKE@
@VARIABLES@

CXXFLAGS=-I../../include/visit @CXXFLAGS@
CPPFLAGS=-I. @CPPFLAGS@ 

##
## Files...
##
SRC= \
    ApplyOperatorRPC.C \
    ClearCacheRPC.C \
    CloneNetworkRPC.C \
    DefineVirtualDatabaseRPC.C \
    ExecuteRPC.C \
    MakePlotRPC.C \
    OpenDatabaseRPC.C \
    PickRPC.C \
    QueryRPC.C \
    ReadRPC.C \
    ReleaseDataRPC.C \
    RenderRPC.C \
    SetWinAnnotAttsRPC.C \
    StartPickRPC.C \
    UpdatePlotAttsRPC.C \
    UseNetworkRPC.C

HDR=$(SRC:.C=.h)

OBJ=$(SRC:.C=.o)
LIB=libenginerpc
LIBS=-lmisc -lstate


##
## Standard targets...
##
all: $(LIBSHARED)

clean:
	$(RM) $(LIBSHARED)
	$(RM) $(OBJ)


##
## Automatic dependency stuff
##
@TARGETS@
@LIBTARGETS@
