# Check to see if the --disable-gui configure flag was given. If
# if was then we will use the enable_gui variable later on to see
# if we should test for X. We will also not build the Gui or the
# viewer.
#
{ echo "$as_me:$LINENO: checking if GUI features are enabled" >&5
echo $ECHO_N "checking if GUI features are enabled... $ECHO_C" >&6; }
# Check whether --enable-gui was given.
if test "${enable_gui+set}" = set; then
  enableval=$enable_gui; UseGUI="$enable_gui"
else
  UseGUI=yes
fi

if test "$UseGUI" = "yes"; then
    { echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6; }
    enable_gui=yes

                { echo "$as_me:$LINENO: checking for viewer" >&5
echo $ECHO_N "checking for viewer... $ECHO_C" >&6; }
    if test -d viewer; then
        { echo "$as_me:$LINENO: result: found" >&5
echo "${ECHO_T}found" >&6; }
        VIEWER="viewer"
        VISWINDOW="VisWindow"
        VIEWERLIB="$""(VDSO)"
        VIEWEROBJ="$""(VOBJ)"
        # The private source can go into the executable on all UNIX versions
        # except for MacOS X.
        if test "$SHORT_UNAME" = "darwin" ; then
            VIEWERLIB_SRC="$""(PUBLIC_SRC) $""(PRIVATE_SRC) $""(SRCMOC)"
            VIEWEREXE_SRC="$""(MAINSRC)"
        else
            VIEWERLIB_SRC="$""(PUBLIC_SRC)"
            VIEWEREXE_SRC="$""(MAINSRC) $""(PRIVATE_SRC) $""(SRCMOC)"
        fi

        if test -z "$VTK"; then
            { { echo "$as_me:$LINENO: error: viewer is here, but no VTK library was specified.
The viewer cannot be built without VTK.  Make sure the VTK environment variable
is set and rerun 'configure'." >&5
echo "$as_me: error: viewer is here, but no VTK library was specified.
The viewer cannot be built without VTK.  Make sure the VTK environment variable
is set and rerun 'configure'." >&2;}
   { (exit 1); exit 1; }; }
        fi
    else
        { echo "$as_me:$LINENO: result: not found" >&5
echo "${ECHO_T}not found" >&6; }
        VIEWER=""
        VISWINDOW=""
        VIEWERLIB_SRC=""
        VIEWEREXE_SRC=""
    fi

                { echo "$as_me:$LINENO: checking for gui" >&5
echo $ECHO_N "checking for gui... $ECHO_C" >&6; }
    if test -d gui; then
        { echo "$as_me:$LINENO: result: found" >&5
echo "${ECHO_T}found" >&6; }
        GUI="gui"
        GUILIB="$""(GDSO)"
        GUIOBJ="$""(GOBJ)"
        if test "$SHORT_UNAME" = "darwin"; then
            GUIFRAMEWORKS="-framework Carbon"
        else
            GUIFRAMEWORKS=""
        fi
        if test -z "$METADATA_SERVER"; then
            { { echo "$as_me:$LINENO: error: gui is here, but the metadata server is not.
The gui will not run without the metadata server. Check to see that the
mdserver directory exists and rerun 'configure'." >&5
echo "$as_me: error: gui is here, but the metadata server is not.
The gui will not run without the metadata server. Check to see that the
mdserver directory exists and rerun 'configure'." >&2;}
   { (exit 1); exit 1; }; }
        fi
    else
        { echo "$as_me:$LINENO: result: not found" >&5
echo "${ECHO_T}not found" >&6; }
        GUI=""
        GUIOBJ=""
        GUIFRAMEWORKS=""
    fi

    ENGINEPROXYDIR="proxy"
else
    { echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6; }
    enable_gui=no
    ENGINEPROXYDIR=""
    GUI_CXXFLAGS=""
fi












