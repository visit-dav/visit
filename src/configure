                    3.0.4  3.0.3  3.0.2  3.0.1  3.0.0 \
                    2.3.2  2.3.1  2.3.0 ; do
            QtVer=`grep \"$ver\" $QtGlobalInc`
            if test -n "$QtVer"; then
                QtVersion=$ver
                break
            fi
        done
    fi

    if test -z "$QtVersion"; then
        { { echo "$as_me:$LINENO: error:   Sorry, but you need Qt version 2.3.0 or higher to compile VisIt. " >&5
echo "$as_me: error:   Sorry, but you need Qt version 2.3.0 or higher to compile VisIt. " >&2;}
   { (exit 1); exit 1; }; }
    else
        { echo "$as_me:$LINENO: result: Qt $QtVersion in $QT_INCLUDE" >&5
echo "${ECHO_T}Qt $QtVersion in $QT_INCLUDE" >&6; }
    fi

    if test -f $QT_LIB/libqt-mt.so -o -f $QT_LIB/libqt-mt.dylib; then
	{ echo "$as_me:$LINENO: result: Linking to libqt-mt" >&5
echo "${ECHO_T}Linking to libqt-mt" >&6; }
	LIBQT=libqt-mt
    	QT_LIBS=-lqt-mt
    else
	{ echo "$as_me:$LINENO: result: Linking to libqt" >&5
echo "${ECHO_T}Linking to libqt" >&6; }
	LIBQT=libqt
    	QT_LIBS=-lqt
    fi






    QUI_LIBS=-lqui

