      echo "#endif" >> include/visit-config.h
      echo "#if defined (__APPLE__) && defined (__BIG_EDIAN__) && !defined (WORDS_BIGENDIAN)" >> include/visit-config.h
      echo "#define WORDS_BIGENDIAN 1" >> include/visit-config.h
      echo "#endif" >> include/visit-config.h
