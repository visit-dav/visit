                if test -z "$QT_BIN" || test -z "$QT_INCLUDE" || test -z "$QT_LIB"; then
       { { echo "$as_me:$LINENO: error: The QT_BIN, QT_INCLUDE and QT_LIB environment variables must be set in order to find Qt" >&5
echo "$as_me: error: The QT_BIN, QT_INCLUDE and QT_LIB environment variables must be set in order to find Qt" >&2;}
   { (exit 1); exit 1; }; }
    fi
    { echo "$as_me:$LINENO: checking for supported Qt version" >&5
echo $ECHO_N "checking for supported Qt version... $ECHO_C" >&6; }
    QtVersion=""
    QtGlobalInc="$QT_INCLUDE/qglobal.h"

    if test -f $QtGlobalInc; then
        ## Check for supported versions of Qt.
	for ver in  3.3.0  3.3.1  3.3.2  3.3.3  3.3.4 3.3.7 3.3.8 \
                    3.2.0  3.2.1  3.2.2  3.2.3 \
