    { echo "$as_me:$LINENO: result: yes" >&5
echo "${ECHO_T}yes" >&6; }

                { echo "$as_me:$LINENO: checking for mdserver" >&5
echo $ECHO_N "checking for mdserver... $ECHO_C" >&6; }
    if test -d mdserver; then
        { echo "$as_me:$LINENO: result: found" >&5
echo "${ECHO_T}found" >&6; }
        METADATA_SERVER="mdserver"
        MDSERVERLIB="$""(MDSO)"
        MDSERVEROBJ="$""(MOBJ)"
    else
        { echo "$as_me:$LINENO: result: not found" >&5
echo "${ECHO_T}not found" >&6; }
        METADATA_SERVER=""
    fi
else
    { echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6; }
    METADATA_SERVER=""
fi




